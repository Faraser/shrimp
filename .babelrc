{
  "stage": 0,
  "plugins": [
    "react-transform"
  ],
  "optional": ["es7.decorators"],
  "extra": {
    "react-transform": [{
      "target": "react-transform-webpack-hmr",
      "imports": ["react"],
      "locals": ["module"]
    }, {
      "target": "react-transform-catch-errors",
      "imports": ["react", "redbox-react"]
    }]
  }
}
