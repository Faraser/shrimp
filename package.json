{
  "name": "project-template",
  "version": "0.0.0",
  "description": "webpack project template",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "NODE_ENV=production webpack --config webpack-production.config.js --progress --profile --colors",
    "webpack-dev": "webpack-dev-server --config webpack-dev.config.js",
    "server-dev": "NODE_ENV=development NODE_PATH=./node_modules:./app babel-node server/server",
    "server": "NODE_ENV=production NODE_PATH=./node_modules:./app babel-node server/server",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run webpack-dev\" \"npm run server-dev\"",
    "master": "npm run webpack && npm run server"
  },
  "author": "Samoilow Maxim <nitive@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^0.1.1",
    "webpack": "^1.12.1"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.18.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.3.2",
    "react": "^0.13.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "webpack-dev-server": "^1.10.1"
  }
}
